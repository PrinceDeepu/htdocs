{
    "name": "Test",
    "version": "0.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://web.classplusapp.com/*"
            ],
            "css": [],
            "js": [
                "/scripts/aspire.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "images/logo-128x128.png"
        }
    },
    "icons": {
        "128": "images/logo-128x128.png"
    },
    "permissions":[
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
} 